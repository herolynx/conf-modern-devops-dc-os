{
    "id": "dcos-java-sample",
    "container": {
      "type": "DOCKER",
      "docker": {
        "image": "mwrona/dcos-java-sample:0.0.1",
        "network": "BRIDGE",
        "portMappings": [
          { "hostPort": 0, "containerPort": 8080, "servicePort": 10010 }
        ]
      }
    },
    "cpus": 0.25,
    "instances": 2,
    "mem": 256,
    "healthChecks": [
      {
        "gracePeriodSeconds": 90,
        "intervalSeconds": 5,
        "maxConsecutiveFailures": 3,
        "path": "/probe/health",
        "portIndex": 0,
        "protocol": "HTTP",
        "timeoutSeconds": 5
      }
    ],
    "labels":{
      "HAPROXY_DEPLOYMENT_GROUP":"dcos-java-sample",
      "HAPROXY_DEPLOYMENT_ALT_PORT":"10011",
      "HAPROXY_GROUP":"external",
      "HAPROXY_0_REDIRECT_TO_HTTPS":"true",
      "HAPROXY_0_VHOST": "192.168.65.111"
    }
}